{
  "name": "blog",
  "description": "File based routing and MDX support Vite.js tooling. Prettier code formatter.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.ssr.tsx",
    "dev": "npm run dev.ssr",
    "dev.client": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --force --mode ssr",
    "dev.ssr": "VITE_NODE_STINKS=true node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "start": "npm run dev",
    "typecheck": "tsc --incremental --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.0.107",
    "@builder.io/qwik-city": "^0.0.107",
    "@types/eslint": "8.4.6",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "@typescript-eslint/parser": "5.34.0",
    "eslint": "8.22.0",
    "eslint-plugin-qwik": "0.0.105",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vite-tsconfig-paths": "3.5.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "type": "module"
}
